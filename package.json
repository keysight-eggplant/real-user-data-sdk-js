{
  "name": "@eggplantio/real-user-data-sdk-js",
  "version": "2.0.3",
  "description": "SDK for generating and publishing events that capture user, application and technical data",
  "main": "dist/rci.min.js",
  "scripts": {
    "test": "node node_modules/jest/bin/jest.js src && npm run build && node node_modules/karma/bin/karma start",
    "eslint": "node node_modules/eslint/bin/eslint.js ./src ./examples/Vanilla --fix",
    "build": "node node_modules/webpack/bin/webpack.js --config=config/webpack.js --env=scope=core --env=rulesTarget=current --env=mode=production",
    "build:legacy": "node node_modules/webpack/bin/webpack.js --config=config/webpack.js --env=scope=core --env=rulesTarget=legacy --env=mode=production",
    "build:dev": "node node_modules/webpack/bin/webpack.js --config=config/webpack.js --env=scope=core --env=rulesTarget=current --env=mode=development",
    "build:legacy:dev": "node node_modules/webpack/bin/webpack.js --config=config/webpack.js --env=scope=core --env=rulesTarget=legacy --env=mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TestPlant/real-user-data-sdk-js.git"
  },
  "keywords": [],
  "author": "Eggplant - RCI team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TestPlant/real-user-data-sdk-js/issues"
  },
  "homepage": "https://github.com/TestPlant/real-user-data-sdk-js#readme",
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.2",
    "@babel/runtime": "7.4.3",
    "@babel/runtime-corejs2": "7.8.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "24.5.0",
    "babel-loader": "8.0.5",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "jest": "26.6.3",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "minimist": "1.2.3",
    "mocha": "^6.1.4",
    "npm-cli-login": "^0.1.1",
    "sinon": "12.0.1",
    "webpack": "5.64.1",
    "webpack-cli": "4.9.1"
  },
  "dependencies": {
    "ua-parser-js": "^0.7.24"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
